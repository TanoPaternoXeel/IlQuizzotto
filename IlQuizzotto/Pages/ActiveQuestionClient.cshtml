@page
@model IlQuizzotto.Pages.ActiveQuestionClinetModel
@{
      ViewData["Title"] = "Quizzotto ";
}
<h1>Quizzotto</h1>
<div class="container">
    <input type="text" id="querrentQuestion" />
</div>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
<script src="~/js/signalr.js"></script>
<script>
    "use strict";

var connection = new signalR.HubConnectionBuilder().withUrl("/quizzottoHub").build();

//Disable the send button until connection is established.


connection.on("ReceiveMessage", function (question) {
   $("#querrentQuestion").val(question);
});

connection.start().then(function () {
  debugger;
}).catch(function (err) {
    return console.error(err.toString());
});


</script>